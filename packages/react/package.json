{
  "name": "@token-icons/react",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./index.ts"
    }
  },
  "scripts": {
    "build": "tsup ./index.ts --format cjs,esm --dts",
    "dev": "bun run build -- --watch",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,json}\""
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "bun-types": "latest",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/token-icons/monorepo",
    "directory": "packages/react"
  },
}
