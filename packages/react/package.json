    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/0xa3k5/token-icons",
    "directory": "packages/react"
  }
