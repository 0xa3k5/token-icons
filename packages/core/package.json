{
  "name": "@token-icons/core",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && bun run optimize-svgs && bun run generate-core:index && bun run enrich-metadata && bun generate-react",
    "optimize-svgs": "bun run ./scripts/optimize-svgs.ts",
    "generate-core:index": "bun run ./scripts/generate-core-index.ts",
    "generate-react": "bun run generate-react:components && bun run generate-react:index",
    "generate-react:components": "bun run ./scripts/generate-react-components.ts",
    "generate-react:index": "bun run ./scripts/generate-react-index.ts",
    "enrich-metadata": "bun run ./scripts/enrich-metadata.ts"
  },
  "devDependencies": {
    "bun-types": "latest",
    "cheerio": "^1.0.0-rc.12",
    "svgo": "^3.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/token-icons/monorepo",
    "directory": "packages/core"
  }
}
